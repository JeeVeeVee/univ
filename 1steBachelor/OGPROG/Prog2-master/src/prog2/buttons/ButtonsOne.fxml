<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<AnchorPane id="AnchorPane" prefHeight="171.0" prefWidth="319.0" styleClass="mainFxmlClass"
            xmlns:fx="http://javafx.com/fxml" fx:controller="prog2.buttons.ButtonsOneCompanion">
    <stylesheets>
        <URL value="@simple.css"/>
    </stylesheets>
    <Label fx:id="blzLabel" alignment="CENTER" contentDisplay="TEXT_ONLY" layoutX="160.0" layoutY="14.0"
           prefHeight="143.0" prefWidth="146.0" text="Blz 1"/>
    <Button layoutX="36.0" layoutY="14.0" mnemonicParsing="false" onAction="#doMinus10" text="- 10"/>
    <Button layoutX="36.0" layoutY="55.0" mnemonicParsing="false" onAction="#doMinus1"  text="- 1"/>
    <Button layoutX="36.0" layoutY="94.0" mnemonicParsing="false" onAction="#doPlus1"  text="+ 1"/>
    <Button layoutX="36.0" layoutY="130.0" mnemonicParsing="false" onAction="#doPlus10" text="+ 10"/>
</AnchorPane>
