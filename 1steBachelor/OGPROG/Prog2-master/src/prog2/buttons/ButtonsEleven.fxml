<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<AnchorPane id="AnchorPane" prefHeight="171.0" prefWidth="319.0" styleClass="mainFxmlClass"
            xmlns:fx="http://javafx.com/fxml" fx:controller="prog2.buttons.ButtonsElevenCompanion">
  <stylesheets>
    <URL value="@disabledEnabled.css"/>
  </stylesheets>
  <Label fx:id="blzLabel" alignment="CENTER" contentDisplay="TEXT_ONLY" layoutX="160.0" layoutY="14.0"
         prefHeight="143.0" prefWidth="146.0" text="Blz 1"/>
  <Button userData="-10" fx:id="minus10" layoutX="36.0" layoutY="14.0" mnemonicParsing="false" onAction="#doButton" text="Min 10"/>
  <Button userData="-1" fx:id="minus1" layoutX="36.0" layoutY="55.0" mnemonicParsing="false" onAction="#doButton" text="Min 1"/>
  <Button userData="1" fx:id="plus1" layoutX="36.0" layoutY="94.0" mnemonicParsing="false" onAction="#doButton" text="Plus 1"/>
  <Button userData="10" fx:id="plus10" layoutX="36.0" layoutY="130.0" mnemonicParsing="false" onAction="#doButton" text="Plus 10"/>
</AnchorPane>
